{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "composite": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "jsx": "preserve"
  }
}
